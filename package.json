{
  "name": "react-agent-prototype",
  "version": "1.0.0",
  "description": "ReAct AI Agent Prototype - CMS-Focused",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"pnpm:dev:server\" \"pnpm:dev:preview\" \"pnpm:dev:web\"",
    "dev:server": "tsx watch server/index.ts",
    "dev:preview": "tsx watch server/preview.ts",
    "dev:web": "next dev -p 3000",
    "preview": "open http://localhost:4000/pages/home?locale=en",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "seed": "tsx scripts/seed.ts",
    "reindex": "tsx scripts/reindex-vector.ts",
    "format": "biome format --write .",
    "lint": "biome check .",
    "typecheck": "tsc --noEmit",
    "build": "next build",
    "start": "next start"
  },
  "keywords": [
    "react",
    "agent",
    "cms",
    "ai",
    "next.js"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@ai-sdk/react": "^2.0.89",
    "@lancedb/lancedb": "^0.22.3",
    "@openrouter/ai-sdk-provider": "^1.2.1",
    "@tailwindcss/typography": "^0.5.19",
    "@tanstack/react-query": "^5.90.7",
    "ai": "^5.0.89",
    "autoprefixer": "^10.4.21",
    "better-sqlite3": "^12.4.1",
    "cors": "^2.8.5",
    "drizzle-orm": "^0.44.7",
    "drizzle-zod": "^0.8.3",
    "express": "^5.1.0",
    "marked": "^17.0.0",
    "multer": "^2.0.2",
    "next": "^16.0.1",
    "nunjucks": "^3.2.4",
    "postcss": "^8.5.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "sonner": "^2.0.7",
    "tailwindcss": "^4.1.17",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.4",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/marked": "^6.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.10.0",
    "@types/nunjucks": "^3.2.6",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.6",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
