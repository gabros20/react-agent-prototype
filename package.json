{
	"name": "react-agent-prototype",
	"version": "1.0.0",
	"description": "ReAct AI Agent Prototype - CMS with AI-Powered Image Management",
	"private": true,
	"type": "module",
	"scripts": {
		"start": "concurrently \"pnpm:dev:server\" \"pnpm:dev:preview\" \"pnpm:dev:web\" \"pnpm:dev:worker\"",
		"start:redis": "tsx scripts/start-redis.ts",
		"start:all": "tsx scripts/start-all.ts",
		"stop": "tsx scripts/stop-dev.ts",
		"stop:redis": "tsx scripts/stop-redis.ts",
		"stop:all": "tsx scripts/stop-all.ts",
		"restart": "pnpm stop && pnpm start",
		"status": "tsx scripts/status.ts",
		"ps": "tsx scripts/ps.ts",
		"dev": "pnpm start",
		"dev:server": "tsx watch server/index.ts",
		"dev:preview": "tsx watch server/preview.ts",
		"dev:web": "next dev -p 3000",
		"dev:worker": "tsx watch scripts/start-worker.ts",
		"preview": "open http://localhost:4000/pages/home?locale=en",
		"db:generate": "drizzle-kit generate",
		"db:push": "drizzle-kit push",
		"db:studio": "drizzle-kit studio",
		"seed": "tsx scripts/seed.ts",
		"seed:images": "tsx scripts/seed-images.ts",
		"check:images": "tsx scripts/check-images.ts",
		"reset:system": "tsx scripts/reset-system.ts",
		"reset:complete": "tsx scripts/reset-complete.ts",
		"reset:data": "tsx scripts/reset-data-only.ts",
		"verify": "tsx scripts/verify-system.ts",
		"reindex": "tsx scripts/reindex-vector.ts",
		"generate:placeholders": "tsx scripts/generate-placeholders.ts",
		"worker": "tsx scripts/start-worker.ts",
		"retry:images": "tsx scripts/retry-failed-images.ts",
		"format": "biome format --write .",
		"lint": "biome check .",
		"typecheck": "tsc --noEmit",
		"build": "next build",
		"prod": "next start"
	},
	"keywords": [
		"react",
		"agent",
		"cms",
		"ai",
		"next.js"
	],
	"author": "",
	"license": "ISC",
